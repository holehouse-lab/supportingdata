   "source": [
    "import numpy as np\n",
    "from camparitraj.cttrajectory import CTTrajectory\n",
    "\n",
    "CT = CTTrajectory('full.pdb','full.xtc')\n",
    "CP = CT.proteinTrajectoryList[0]\n",
    "\n",
    "e2e = CP.get_end_to_end_distance()\n",
    "\n",
    "np.savetxt('end_to_end_distance.csv', e2e)"
   ]
  }
